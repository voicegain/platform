{
        "host" : "${apiUrlBase}",
        "ws": "${natsWebsocketUrlBase}/port",
        "micrometer" : "${micrometerUrl}",
        "jwt" : "${jwt}",
        "payloadWS" : {
          "messages" : [
          {
            "text": "Transcription will start soon ...",
            "tags": [
              "announce"
            ]
          }
        ]
        } ,
        "payloadASR" : {
              "sessions": [{
                      "asyncMode" : "REAL-TIME",
                      "websocket": {
                              "name": "${websocketName}",
                              "label" : "${websocketName}-DATE",
                              "minimumDelay" : 2000,
                              "ifExists" : "kill"
                      },
                      "poll" : {
                              "afterlife" :  15000,
                              "persist" : -1
                      }
              }],
              "audio": {
                      "source": {
                              "stream": {
                                      "protocol": "ASCALON",
                                      "controlWebsocket": "${websocketName}"
                              }
                      },
                      "rate" : 16000,
                      "channels" : "mono",
                      "format" : "L16",
                      "capture" : true
              },
              "settings": {
                      "preemptible" : false,
                      "asr": {
                              "noInputTimeout" : -1,
                              "completeTimeout" : -1
                      }
              }
        }
}